{
    "compilerOptions": {
      "module": "ESNext",
      "target": "ESNext",
      "moduleResolution": "Bundler",
      "baseUrl": ".",
      "paths": {
        "@/*": ["./src/*"]
      },
      "strict": true,
      "jsx": "preserve",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "types": ["vite/client"]
    },
    "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue"],
    // "references": [{ "path": "./tsconfig.node.json" }]
  }
  